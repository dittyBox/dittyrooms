namespace MOM.MES.Entity
{
    public partial class Carrier
    {
        /// <summary>
        /// Override object.ToString()
        /// </summary>
        /// <returns></returns>
        public override string ToString()
        {
            return string.Format("Carrierid={0} State={1} Loadstate={2} Assignqty={3}",
                this.Carrierid,
                this.State,
                this.Loadstate,
                this.Assignqty);
        }
    }

    public partial class Durable
    {
        /// <summary>
        /// Override object.ToString()
        /// </summary>
        /// <returns></returns>
        public override string ToString()
        {
            return string.Format("Durableid={0} State={1} Location={2} Usagecount={3} Cleancount={4}",
                this.Durableid,
                this.State,
                this.Location,
                this.Usagecount,
                this.Cleancount);
        }
    }

